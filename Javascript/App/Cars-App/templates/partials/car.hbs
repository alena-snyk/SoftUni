<div class="listing">
    <p>{{car.title}}</p>
    <img src={{car.imageUrl}}>
    <h2>Brand: {{car.brand}}</h2>
    <div class="info">
        <div id="data-info">
            <h3>Seller: {{seller}}</h3>
            <h3>Fuel: {{fuel}}</h3>
            <h3>Year: {{year}}</h3>
            <h3>Price: {{price}} $</h3>
        </div>
        <div id="data-buttons">
            <ul>
                <li class="action">
                    <a href="#car/details" class="button-carDetails">Details</a>
                </li>
                <li class="action">
                    <a href="#car/edit" class="button-carDetails">edit</a>
                </li>
                <li class="action">
                    <a href="#car/remove" class="button-carDetails">delete</a>
                </li>
            </ul>
        </div>
    </div>
</div>